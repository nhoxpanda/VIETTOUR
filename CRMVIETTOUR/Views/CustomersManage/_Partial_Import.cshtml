<div class="modal fade bs-modal-lg" id="modal-import-customer" role="document" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">Import khách hàng</h4>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-8">
                            <input type="file" name="Import" id="fileImport" class="form-control" style="width:250px; float:left; padding:2.5px 1px" />
                            <button class="btn btn-danger" id="btnRemove" onclick="DeleteImport()"><i class="fa fa-times"></i>&nbsp;Xóa</button>
                            <input type="hidden" id="listItemIdI" />
                            <button class="btn yellow" onclick="SaveImport()"><i class="fa fa-floppy-o"></i>&nbsp;Lưu</button>
                            <button class="btn grey" data-dismiss="modal"><i class="fa fa-ban"></i>&nbsp;Đóng</button>
                            <a class="btn green" data-toggle="modal" href="~/CustomersManage/ExcelSample"><i class="fa fa-file-text"></i>&nbsp;File Mẫu</a>
                        </div>
                        <label class="col-md-2">
                            <input type="radio" name="CheckImport" value="0" /> Thêm mới
                        </label>
                        <label class="col-md-2">
                            <input type="radio" name="CheckImport" value="1" /> Cập nhật
                        </label>
                        <div class="col-md-12 margintop10" id="import-data-customer">
                            @*import_data_customer*@
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
</div>

