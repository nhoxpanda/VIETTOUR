@using CRM.Core
@model IEnumerable<tbl_Staff>

<div class="table-scrollable table-custom-format">
    <table class="table table-striped table-bordered table-hover" id="">
        <thead>
            <tr>
                <th>Mã số</th>
                <th>Họ tên</th>
                <th>Chức vụ</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="odd gradeX">
                    <td>@item.Code</td>
                    <td>@item.FullName</td>
                    <td>@(item.PositionId != null ? item.tbl_DictionaryPosition.Name : "")</td>
                </tr>
            }
        </tbody>
    </table>
</div>
